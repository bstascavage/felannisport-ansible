# /etc/fstab: static file system information.
#
# Use 'blkid' to print the universally unique identifier for a
# device; this may be used with UUID= as a more robust way to name devices
# that works even if disks are added and removed. See fstab(5).
#
# <file system> <mount point>   <type>  <options>       <dump>  <pass>
# / was on /dev/sda1 during installation
UUID=45c4d705-104e-415f-aede-579bf7c0c45f /               ext4    errors=remount-ro 0       1
# swap was on /dev/sda5 during installation
UUID=5befd6db-daa7-4e30-b6a0-09dd93f6c5e1 none            swap    sw              0       0

/dev/torrent_vg/torrent /storage/complete_downloads ext4 defaults 1 2
/dev/moviesvg/lv_movies /storage/Downloads/Movies ext4 defaults 1 2
192.168.1.240:/volume1/Movies /storage/Movies nfs      rw,sync,hard,intr  0     0
192.168.1.240:/volume1/TV_Shows /storage/TV_Shows nfs      rw,sync,hard,intr  0     0
192.168.1.240:/volume1/backups /storage/backups nfs      rw,sync,hard,intr  0     0
192.168.1.240:/volume1/owncloud /storage/owncloud nfs      rw,users,sync,hard,intr  0     0
192.168.1.240:/volume1/photo /storage/Photos nfs      rw,sync,hard,intr  0     0
192.168.1.240:/volume1/influxdb /storage/influxdb nfs      rw,users,sync,hard,intr  0     0
192.168.1.240:/volume1/opt /storage/opt nfs      rw,users,sync,hard,intr  0     0
